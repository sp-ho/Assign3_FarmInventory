<Window x:Class="FarmInventory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FarmInventory"
        mc:Ignorable="d"
        Title="MainWindow" Height="281" Width="420">
    <Grid>
        <Grid HorizontalAlignment="Center" Height="172" Margin="0,23,0,0" VerticalAlignment="Top">
            <Button x:Name="btnProduct" Content="Admin" HorizontalAlignment="Center" Margin="0,63,0,0" VerticalAlignment="Top" FontSize="36" Click="btnProduct_Click" RenderTransformOrigin="0.118,0.039" Width="168"/>
            <Button x:Name="btnSales" Content="Sales" HorizontalAlignment="Center" Margin="0,120,0,0" VerticalAlignment="Top" FontSize="36" Click="btnSales_Click" Width="168"/>
            <Label Content="HAPPY FARM" HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="24"/>
        </Grid>
    </Grid>
</Window>
